<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginS</servlet-name>
        <servlet-class>servlets.loginS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>servlets.loginS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroServlet</servlet-name>
        <servlet-class>servlets.registroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editarServlet</servlet-name>
        <servlet-class>servlets.editarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcarroServlet</servlet-name>
        <servlet-class>servlets.AcarroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>McarroServlet</servlet-name>
        <servlet-class>servlets.McarroServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginS</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroServlet</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editarServlet</servlet-name>
        <url-pattern>/editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AcarroServlet</servlet-name>
        <url-pattern>/carroS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>McarroServlet</servlet-name>
        <url-pattern>/Mcarro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
